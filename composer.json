{
    "name": "mannysoft/api-form-request",
    "description": "Laravel package for  extending FormRequest for REST API Request.",
    "keywords": [
        "laravel",
        "api",
        "validation"
    ],
    "authors": [
        {
            "name": "Manny Isles"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-0": {
            "Mannysoft\\ApiFormRequest\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mannysoft\\ApiFormRequest\\ServiceProvider"
            ],
            "aliases": {
                "ApiFormRequest": "Mannysoft\\ApiFormRequest\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
